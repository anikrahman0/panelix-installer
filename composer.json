{
    "name": "panelix/installer",
    "description": "Installer for Panelix Laravel Admin Dashboard (Laravel 12 ready)",
    "type": "library",
    "license": "MIT",
    "bin": [
        "bin/panelix"
    ],
    "require": {
        "php": ">=8.2",
       "symfony/console": "^7.3",
        "symfony/process": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Panelix\\Installer\\": "src/"
        }
    },
    "archive": {
        "exclude": [
            "README.md"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
